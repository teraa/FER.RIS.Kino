{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore": "Warning"
      }
    },
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.Seq"
    ],
    "WriteTo": {
      "Console": {
        "Name": "Console"
      },
      "Seq": {
        "Name": "Seq"
      }
    },
    "Enrich": [
      "FromLogContext"
    ]
  },
  "AllowedHosts": "*",
  "Db": {
    "ConnectionString": ""
  },
  "Jwt": {
    "SigningKey": "",
    "TokenLifetime": "",
    "ClockSkew": ""
  }
}
